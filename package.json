{
  "name": "influx-kapacitor",
  "version": "0.8.6",
  "description": "An influx broadcaster supporting multiple transmission protocols and applies backpressure to reduce chattiness.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json --detectOpenHandles && codecov -t fada1a38-28d6-4248-828e-179342c940fd",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build && npm run lint",
    "prepublishOnly": "npm test",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "influx",
    "kapacitor",
    "capacitor",
    "backpressure"
  ],
  "author": "Steve Hebert",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stevehebert/influx-kapacitor/issues"
  },
  "homepage": "https://github.com/stevehebert/influx-kapacitor/",
  "devDependencies": {
    "codecov": "^3.1.0",
    "jest": "^23.6.0",
    "prettier": "^1.13.2",
    "ts-jest": "^22.4.6",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^3.3.3"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@types/request": "^2.48.1",
    "json-to-line-protocol": "^0.2.0",
    "request": "^2.88.0",
    "rxjs": "^6.4.0"
  }
}
